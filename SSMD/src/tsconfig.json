{
    "name" : "SSMD",
    "version" : "0.0.0",
    "author": "SSJ",
    "compilerOptions": {
        "module": "system",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "target": "ES5",
        "inlineSourceMap": true,
        "out":"../dist/module.js",
        "experimentalDecorators":true
    },
    "exclude": [
        "typings"
    ]
}